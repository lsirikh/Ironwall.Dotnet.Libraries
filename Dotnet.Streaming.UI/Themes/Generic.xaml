<ResourceDictionary
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:local="clr-namespace:Dotnet.Streaming.UI"
	xmlns:views="clr-namespace:Dotnet.Streaming.UI.Views">


	<Style TargetType="{x:Type views:AiScreenView}">
		<Setter Property="Template">
			<Setter.Value>
				<ControlTemplate TargetType="{x:Type views:AiScreenView}">
					<Grid>
						<Image
							x:Name="PART_VideoImage"
							SnapsToDevicePixels="True"
							UseLayoutRounding="True" />
						<Canvas x:Name="PART_Overlay" IsHitTestVisible="False" />
					</Grid>
				</ControlTemplate>
			</Setter.Value>
		</Setter>
	</Style>
</ResourceDictionary>
